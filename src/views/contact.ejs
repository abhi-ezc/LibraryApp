    <section class="container">
        <section class="custom-container-index">
            <div class="custom-section-body">
                <h2>ABOUT US</h2>
                <div>
                    <form id="form1">
                        <div class="form-group">
                            <!-- Name -->
                            <label class="form-text">Name(Required)</label>
                            <input class="form-control" type="text" id="name" placeholder="Name" onblur="validateName()" required>
                            <label class="validateLabel" id="reqContactName"></label>
                            <br>
                            <!-- Email -->
                            <label class="form-text">Email(Required)</label>
                            <input class="form-control" type="text" id="email" placeholder="Email" onblur="validateEmail()" required>
                            <label class="validateLabel" id="reqContactEmail"></label>
                            <br>
                            <!-- Mobile -->
                            <label class="form-text">Mobile(Required)</label>
                            <input class="form-control" type="text" id="mobile" placeholder="Mobile" onblur="validateMobile()" required>
                            <label class="validateLabel" id="reqContactMobile"></label>
                            <br>
                            <!-- Message -->
                            <label class="form-text">Message</label>
                            <textarea class="form-control"></textarea>
                            <br>
                            <!-- button -->
                            <input type="button" class="btn btn-primary" value="submit" onclick="validateMessage()">
                        </div>
                    </form>


                </div>
            </div>
        </section>

    </section>
    
